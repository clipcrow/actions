{
    "compilerOptions": {
        "lib": ["ES2021"],
        "target": "ES2021",
        "module": "CommonJS",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "jsxImportSource": "jsx-slack"
    }
}
