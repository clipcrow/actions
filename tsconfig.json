{
    "compilerOptions": {
        "lib": ["ES2021"],
        "target": "ES2021",
        "module": "CommonJS",
        "moduleResolution": "node",
        "outDir": "dist",
        "skipLibCheck": true,
        "noImplicitAny": true,
        "strict": true,
        "sourceMap": true,
        "jsx": "react-jsx",
        "jsxImportSource": "jsx-slack"
    },
    "exclude": [
        "node_modules",
        "**/*.test.*",
        "**/*.spec.*"
    ]
}
